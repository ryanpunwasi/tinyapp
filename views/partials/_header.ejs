<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-dark shadow-lg">
    <a class="navbar-brand font-weight-bolder" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link font-weight-bold" href="/urls">URLs</a>
      </div>
    </div>
    <ul class="navbar-nav ml-auto"></ul>
    <% if(!user) { %>
        <form class="nav-item nav-link form-inline" method="GET" action="/login">
          <button class="btn btn-outline-primary font-weight-bold">Log In</button>
        </form>
        <form action="/register" method="GET"><button class="btn btn-outline-danger font-weight-bold">Register</button></form>
      <% } else { %>
      <span class="nav-item nav-link text-light">Welcome, <%= user.email %>!</span>
      <form method="POST" action="/logout" class="nav-item nav-link form-inline"><button class="btn btn-danger">Sign Out</button></form>
      <% } %>
    </ul>
  </nav>
</header>